<launch>
      <param name="/thread_left_vessel/side_h" value="5" type="int" />
      <param name="/thread_left_vessel/variance_h" value="0.9" type="double" />

      <param name="/thread_left_vessel/beta" value="0.37" type="double" />
      <param name="/thread_left_vessel/c" value="0.1" type="double" />
      
      <param name="/thread_left_vessel/side_p" value="3" type="int" />
      <param name="/thread_left_vessel/variance_p" value="1.1" type="double" />

      
      <param name="/thread_right_vessel/side_h" value="5" type="int" />
      <param name="/thread_right_vessel/variance_h" value="0.9" type="double" />

      <param name="/thread_right_vessel/beta" value="0.37" type="double" />
      <param name="/thread_right_vessel/c" value="0.1" type="double" />
      
      <param name="/thread_right_vessel/side_p" value="3" type="int" />
      <param name="/thread_right_vessel/variance_p" value="1.1" type="double" />
  
      <node name="thread_left_vessel" pkg="vesselness_image_filter" type="vesselness_image_filter_gpu_node">
         <remap from="image_in" to="/nurbs_sim/left/image" />
         <remap from="image_thin" to="/nurbs_sim/left/image_thin" />
      </node>
      <node name="thread_right_vessel" pkg="vesselness_image_filter" type="vesselness_image_filter_gpu_node">
         <remap from="image_in" to="/nurbs_sim/right/image" />
         <remap from="image_thin" to="/nurbs_sim/right/image_thin" />
      </node>
      <node name="thread_right_vessel_view" pkg="vesselness_image_filter" type="vesselness_viewer_gpu_node">
         <remap from="image_thin" to="/nurbs_sim/right/image_thin" />
      </node>
      <node name="thread_left_vessel_view" pkg="vesselness_image_filter" type="vesselness_viewer_gpu_node">
         <remap from="image_thin" to="/nurbs_sim/left/image_thin" />
      </node>
      <node name="vessel_settings" pkg="rqt_reconfigure" type="rqt_reconfigure">
      </node>
</launch>